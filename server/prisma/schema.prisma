generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id                    String    @id @default(uuid())
  first_n               String
  last_n                String
  email                 String    @unique
  password              String
  last_password         String?
  last_time_change_pass DateTime?
  phone                 String?
  avatar                String?
  bio                   String?   @db.VarChar(1000)
  country               String?
}
